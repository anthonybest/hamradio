	<STRING>LSB</STRING>
	<STRING>CW-LSB</STRING>
	<STRING>DATA-LSB</STRING>
	<STRING>FSK</STRING>
	<STRING>RTTY-LSB</STRING>
</LSBMODES>

<REPLY>
	<SYMBOL>MODE</SYMBOL>
	<SIZE>5</SIZE>
	<STRING>MD</STRING>
	<DATA>
		<DTYPE>STRING</DTYPE>
		<SIZE>2</SIZE>
	</DATA>
	<STRING>;</STRING>
</REPLY>

<REPLY>
	<SYMBOL>FREQ</SYMBOL>
	<SIZE>12</SIZE>
	<STRING>FA</STRING>
	<DATA>
		<DTYPE>DECIMAL</DTYPE>
		<SIZE>9</SIZE>
		<MAX>075000000</MAX>
		<MIN>000030000</MIN>
		<RESOL>1</RESOL>
		<REV>false</REV>
	</DATA>
	<STRING>;</STRING>
</REPLY>	

<COMMAND>
	<SYMBOL>SETFREQ</SYMBOL>
	<SIZE>12</SIZE>
        <STRING>FA</STRING>
	<DATA>
		<DTYPE>DECIMAL</DTYPE>
		<SIZE> 9 </SIZE>
		<MAX>075000000</MAX>
		<MIN>000030000</MIN>
		<RESOL>1</RESOL>
		<REV>false</REV>
